@model Supplier

<h2> Supplier Details</h2>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Supplier Name
        </dt>
        <dd class="col-sm-10">
            @Model.Name
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Model.Email
        </dd> 
        <dt class="col-sm-2">
            Phone
        </dt>
        <dd class="col-sm-10">
            @Model.Phone
        </dd> 
        <dt class="col-sm-2">
            Password
        </dt>
        <dd class="col-sm-10">
            @Model.Password
        </dd>
       
    </dl>
</div>
<div>
    <a asp-action="Update" asp-route-id="@Model.Id">Edit</a>
    <a asp-action="GetAll">Back to List</a>
</div>
