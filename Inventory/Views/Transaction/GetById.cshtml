@model Transaction

<h2>Product Details</h2>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Transaction Type
        </dt>
        <dd class="col-sm-10">
            @Model.TransactionType
        </dd>
        <dt class="col-sm-2">
            Quantity
        </dt>
        <dd class="col-sm-10">
            @Model.Quantity
        </dd>
        <dt class="col-sm-2">
            Date
        </dt>
        <dd class="col-sm-10">
            @Model.TransactionDate
        </dd>
        <dt class="col-sm-2">
            Stock Quantity
        </dt>
    </dl>
</div>
<div>
    <a asp-action="Update" asp-route-id="@Model.ID">Edit</a> |
    <a asp-action="GetAll">Back to List</a>
</div>
